{
  "name": "ember-container-query",
  "version": "2.0.0",
  "description": "Container queries using Ember modifiers",
  "keywords": [
    "container-queries",
    "container-query",
    "element-queries",
    "element-query",
    "ember-addon",
    "ember-octane",
    "emberjs",
    "responsive-design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-container-query.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:test": "ember build --environment=test",
    "changelog": "lerna-changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:dependency": "ember dependency-lint",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "DEVICE='w3-h3' ember test",
    "test:ember": "ember test",
    "test:ember-compatibility": "./node_modules/.bin/ember try:one"
  },
  "changelog": {
    "labels": {
      "breaking": "Breaking Change",
      "bug": "Bug Fix",
      "enhance: code": "Enhancement",
      "enhance: dependency": "Internal",
      "enhance: documentation": "Documentation",
      "user feedback": "User Feedback"
    }
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-element-helper": "^0.6.0",
    "ember-on-resize-modifier": "^1.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.7.0",
    "@embroider/test-setup": "^1.6.0",
    "@glimmer/component": "^1.1.1",
    "@glimmer/tracking": "^1.1.1",
    "@percy/cli": "^1.0.5",
    "@percy/ember": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "d3-array": "^3.1.5",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "ember-a11y-testing": "^5.0.0",
    "ember-auto-import": "^2.4.1",
    "ember-cli": "~4.3.0",
    "ember-cli-dependency-checker": "^3.3.0",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-netlify": "^0.4.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-css-modules": "^2.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-named-blocks-polyfill": "^0.2.5",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.3.4",
    "ember-template-lint": "^4.3.0",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lerna-changelog": "^2.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "qunit": "^2.18.1",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.72.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://ember-container-query.netlify.app"
  }
}
