{
  "name": "ember-container-query",
  "version": "0.0.0",
  "description": "Container queries using Ember modifiers",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-container-query"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:dependency": "ember dependency-lint",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all --aggregate-output --continue-on-error --parallel test:ember:*",
    "test:ember:w1-h1": "DEVICE='w1-h1' ember test --test-port=7357",
    "test:ember:w2-h1": "DEVICE='w2-h1' ember test --test-port=7358",
    "test:ember:w3-h1": "DEVICE='w3-h1' ember test --test-port=7359",
    "test:ember:w1-h2": "DEVICE='w1-h2' ember test --test-port=7360",
    "test:ember:w2-h2": "DEVICE='w2-h2' ember test --test-port=7361",
    "test:ember:w3-h2": "DEVICE='w3-h2' ember test --test-port=7362",
    "test:ember:w1-h3": "DEVICE='w1-h3' ember test --test-port=7363",
    "test:ember:w2-h3": "DEVICE='w2-h3' ember test --test-port=7364",
    "test:ember:w3-h3": "DEVICE='w3-h3' ember test --test-port=7365",
    "test:ember-compatibility": "ember try:one"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^5.1.2",
    "ember-did-resize-modifier": "^1.0.0",
    "ember-test-selectors": "^4.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.18.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-css-modules": "^1.3.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.7.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.0.0",
    "eslint-plugin-ember": "^8.4.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
