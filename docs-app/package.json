{
  "name": "docs-app",
  "version": "1.0.0",
  "private": true,
  "description": "docs-app for ember-container-query",
  "keywords": [
    "container-queries",
    "container-query",
    "element-queries",
    "element-query",
    "ember-octane",
    "emberjs",
    "glint",
    "responsive-design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-container-query.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:test": "ember build --environment=test",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:css": "stylelint 'app/**/*.css' --cache",
    "lint:css:fix": "stylelint 'app/**/*.css' --fix",
    "lint:dependency": "ember dependency-lint",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prelint:types": "type-css-modules --src app",
    "lint:types": "glint",
    "start": "ember serve --port=4200",
    "test": "DEVICE='w3-h3' ember test --test-port=0",
    "test:device": "ember test"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/broccoli-side-watch": "^1.0.1",
    "@embroider/compat": "^3.7.0",
    "@embroider/core": "^3.4.19",
    "@embroider/macros": "^1.16.9",
    "@embroider/router": "^2.1.8",
    "@embroider/webpack": "^4.0.8",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.0",
    "@glint/environment-ember-loose": "^1.5.0",
    "@glint/environment-ember-template-imports": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@percy/cli": "^1.30.4",
    "@percy/ember": "^4.2.0",
    "@shared-configs/ember-template-lint": "workspace:*",
    "@shared-configs/eslint-config-ember": "workspace:*",
    "@shared-configs/prettier": "workspace:*",
    "@shared-configs/stylelint": "workspace:*",
    "@shared-configs/testem": "workspace:*",
    "@shared-configs/typescript": "workspace:*",
    "@types/qunit": "^2.19.12",
    "autoprefixer": "^10.4.20",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.0",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "ember-a11y-refocus": "^4.1.4",
    "ember-a11y-testing": "^7.0.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~5.11.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-netlify": "^0.4.1",
    "ember-container-query": "workspace:*",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.1.1",
    "ember-resolver": "^13.1.0",
    "ember-source": "~5.11.0",
    "ember-svg-jar": "^2.6.0",
    "ember-template-imports": "^4.2.0",
    "ember-template-lint": "^6.0.0",
    "ember-test-selectors": "^7.0.0",
    "ember-truth-helpers": "^4.0.3",
    "embroider-css-modules": "^2.0.11",
    "eslint": "^8.57.1",
    "loader.js": "^4.7.0",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "qunit": "^2.23.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.11.0",
    "type-css-modules": "^1.0.13",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "ember": {
    "edition": "octane"
  }
}
