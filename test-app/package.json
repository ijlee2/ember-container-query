{
  "name": "test-app",
  "version": "2.1.6",
  "private": true,
  "description": "test-app for ember-container-query",
  "keywords": [
    "container-queries",
    "container-query",
    "element-queries",
    "element-query",
    "ember-octane",
    "emberjs",
    "glint",
    "responsive-design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-container-query.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "scripts": {
    "build": "vite build",
    "build:test": "ember build --environment=test",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "vite --port 4300",
    "test": "vite build --mode test && ember test --config-file \"./testem.cjs\" --path dist --test-port 0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@ember/optional-features": "^3.0.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/compat": "^4.1.13",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.4.3",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.5.1",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.1.1",
    "@glint/template": "^1.7.4",
    "@glint/tsserver-plugin": "^2.1.0",
    "@ijlee2-frontend-configs/ember-template-lint": "^2.3.2",
    "@ijlee2-frontend-configs/eslint-config-ember": "^2.6.1",
    "@ijlee2-frontend-configs/prettier": "^2.4.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@tsconfig/ember": "^3.0.12",
    "@types/qunit": "^2.19.13",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.1",
    "ember-a11y-testing": "^7.1.3",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.10.2",
    "ember-cli-babel": "^8.3.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-container-query": "workspace:*",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.10.1",
    "ember-template-lint": "^7.9.3",
    "ember-test-selectors": "^7.1.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  }
}
