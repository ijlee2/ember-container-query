{
  "name": "test-app",
  "version": "2.1.3",
  "private": true,
  "description": "test-app for ember-container-query",
  "keywords": [
    "container-queries",
    "container-query",
    "element-queries",
    "element-query",
    "ember-octane",
    "emberjs",
    "glint",
    "responsive-design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ijlee2/ember-container-query.git"
  },
  "license": "MIT",
  "author": "Isaac J. Lee",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build:test": "ember build --environment=test",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:dependency": "ember dependency-lint",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "start": "ember serve --port=4300",
    "test": "ember test --test-port=0",
    "test:ember-compatibility": "ember try:one"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/broccoli-side-watch": "^1.1.0",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.8",
    "@ijlee2-frontend-configs/ember-template-lint": "^2.3.0",
    "@ijlee2-frontend-configs/eslint-config-ember": "^2.4.1",
    "@ijlee2-frontend-configs/prettier": "^2.3.1",
    "@tsconfig/ember": "^3.0.12",
    "@types/qunit": "^2.19.13",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "ember-a11y-testing": "^7.1.3",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.8.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-container-query": "workspace:*",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-route-template": "^1.0.3",
    "ember-source": "~6.8.2",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.9.3",
    "ember-test-selectors": "^7.1.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.39.1",
    "latest-version": "^9.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.7.4",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  }
}
